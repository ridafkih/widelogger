{
  "name": "@lab/multiplayer-sdk",
  "version": "0.1.0",
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "types": "tsc --noEmit",
    "lint": "oxlint",
    "format": "oxfmt ."
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "@types/node": "^25.1.0",
    "oxfmt": "0.27.0",
    "oxlint": "1.42.0",
    "typescript": "^5"
  }
}
