{
  "name": "@lab/ui",
  "version": "0.1.0",
  "type": "module",
  "typesVersions": {
    "*": {
      "components/*": [
        "src/components/*"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "hooks/*": [
        "src/hooks/*"
      ]
    }
  },
  "exports": {
    "./theme.css": "./src/theme.css",
    "./components/*": {
      "types": "./src/components/*.tsx",
      "default": "./src/components/*.tsx"
    },
    "./utils/*": {
      "types": "./src/utils/*.ts",
      "default": "./src/utils/*.ts"
    },
    "./hooks/*": {
      "types": "./src/hooks/*.ts",
      "default": "./src/hooks/*.ts"
    }
  },
  "scripts": {
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@lab/typescript-config": "workspace:*",
    "@types/bun": "^1.3.8",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19",
    "lucide-react": "^0.563.0",
    "next": "^16",
    "typescript": "^5"
  },
  "peerDependencies": {
    "lucide-react": ">=0.563.0",
    "next": ">=14",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  }
}
