{
  "name": "@lab/widelogger",
  "version": "0.1.0",
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "types": "tsc --noEmit",
    "lint": "oxlint",
    "format": "oxfmt ."
  },
  "dependencies": {
    "arktype": "^2.1.29",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3"
  },
  "devDependencies": {
    "@lab/typescript-config": "workspace:*",
    "@types/bun": "^1.3.8",
    "oxfmt": "0.28.0",
    "oxlint": "1.43.0",
    "typescript": "^5"
  }
}
